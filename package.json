{
  "name": "designsystem",
  "nativescript": {
    "id": "dk.kirby.design.system",
    "tns-ios": {
      "version": "6.0.1"
    },
    "tns-android": {
      "version": "6.0.0"
    }
  },
  "version": "0.0.264",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build-polyfills": "webpack --config webpack.polyfills.config.js",
    "test": "ng test",
    "ci-test": "ng test --progress false --watch false --code-coverage",
    "lint": "tslint './src/**/*.ts'",
    "lint:fix": "tslint './src/**/*.ts' --fix",
    "e2e": "ng e2e",
    "dist": "ng build --prod --aot --progress false",
    "android": "tns run android",
    "android-debug": "tns debug android",
    "ios": "tns run ios",
    "ios-debug": "tns debug ios --device 'iPhone X'",
    "dist:ios": "tns build ios --for-device --provision dk.kirby.design.system --copy-to dist/designsystem.ipa",
    "dist:android": "tns build android --for-device --copy-to dist/designsystem.apk",
    "pack-copy-to-dist-folder-watch": "cpx \"src\\kirby\\**\\!(*.spec.ts)\" dist-lib --watch",
    "postinstall": "npm run build-polyfills && npm run transpile:tools && npm run transpile:styles",
    "postversion": "git push && git push --tags",
    "coverage-report": "cat ./coverage/lcov.info | coveralls",
    "transpile:tools": "tsc -p tools/tslint-rules",
    "transpile:styles": "sass --style=compressed --no-source-map src/kirby/components/calendar/calendar.webview.scss src/kirby/components/calendar/calendar.webview.css"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "8.2.3",
    "@angular/common": "8.2.3",
    "@angular/compiler": "8.2.3",
    "@angular/core": "8.2.3",
    "@angular/fire": "5.2.1",
    "@angular/forms": "8.2.3",
    "@angular/platform-browser": "8.2.3",
    "@angular/platform-browser-dynamic": "8.2.3",
    "@angular/router": "8.2.3",
    "@ionic/angular": "4.8.0",
    "@nstudio/nativescript-checkbox": "^1.0.0",
    "@types/prismjs": "1.16.0",
    "core-js": "3.2.1",
    "firebase": "6.4.0",
    "highcharts": "7.0.3",
    "moment": "2.24.0",
    "nativescript-angular": "8.2.0",
    "nativescript-feedback": "1.3.11",
    "nativescript-orientation": "2.2.3",
    "nativescript-platform-css": "1.6.10",
    "nativescript-theme-core": "2.0.5",
    "nativescript-ui-chart": "6.0.0",
    "nativescript-ui-listview": "7.0.4",
    "nativescript-webview-interface": "1.4.3",
    "nativescript-windowed-modal": "6.0.0",
    "prismjs": "1.17.1",
    "reflect-metadata": "0.1.13",
    "rxjs": "6.5.2",
    "tns-core-modules": "6.0.6",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.802.2",
    "@angular/cli": "8.2.2",
    "@angular/compiler-cli": "8.2.3",
    "@nativescript/schematics": "0.7.1",
    "@netbasal/spectator": "3.12.0",
    "@types/jasmine": "3.4.0",
    "@types/jasminewd2": "2.0.6",
    "@types/node": "12.7.2",
    "codelyzer": "^5.0.1",
    "coveralls": "3.0.6",
    "cpx": "1.5.0",
    "css-loader": "^3.2.0",
    "is-docker": "2.0.0",
    "jasmine-core": "3.4.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "4.2.0",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "2.1.0",
    "karma-jasmine": "2.0.1",
    "karma-jasmine-html-reporter": "1.4.2",
    "karma-junit-reporter": "1.2.0",
    "karma-spec-reporter": "0.0.32",
    "nativescript": "^6.0.0",
    "nativescript-dev-webpack": "1.1.1",
    "ng-mocks": "8.1.0",
    "node-sass": "4.12.0",
    "prettier": "1.18.2",
    "protractor": "5.4.2",
    "resize-observer-polyfill": "1.5.1",
    "sass": "1.22.10",
    "sass-extract": "2.1.0",
    "sass-extract-loader": "1.1.0",
    "tns-platform-declarations": "6.0.6",
    "ts-node": "8.3.0",
    "tslint": "5.19.0",
    "tslint-plugin-prettier": "2.0.1",
    "typescript": "3.4.5",
    "webpack-cli": "^3.3.7"
  }
}
